@model LibraryManagement.Models.Branch.BranchIndexModel
@{
    ViewData["Title"] = " BranchIndex";
}

<h3>@ViewData["Title"]</h3>
<div id="brachIndex">
    <table class="table table-condensed" id="branchIndexTable">
        <thead>
            <tr>
                <th>Branch Name</th>
                <th>Open Now</th>
                <th>Name</th>
                <th>Number Of Assets</th>
                <th>Number of Patrons</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var branch in Model.Braches)
            {
            <tr>
                <td><a asp-controller="Branch" asp-action="Detail" asp-route-id="@branch.Id">(Profile)</a></td>
                <td>@branch.IsOpen</td>
                <td>@branch.Name</td>
                <td>@branch.NumberOfAssets</td>
                <td>@branch.NumberOfPattorns</td>

            </tr>
            }
        </tbody>
    </table>
</div>

